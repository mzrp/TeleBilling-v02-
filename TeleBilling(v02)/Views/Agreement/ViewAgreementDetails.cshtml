@model TeleBilling_v02_.Models.Agreement

@{
    ViewBag.Title = "ViewAgreementDetails";
}

<h3>@*ViewAgreementDetails*@</h3>



@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div>
        <h3>@Html.DisplayFor(model => model.Description, new { htmlAttributes = new { @disabled = true } })  @Html.DisplayFor(model => model.Status, new { htmlAttributes = new { @class = "form-control" } })</h3>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.Id)

        <table class="table-responsive">

            <tr>
                <td>@Html.LabelFor(model => model.Customer_cvr, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                <td> @Html.DisplayFor(model => model.Customer_cvr, new { htmlAttributes = new { @disabled = true } })</td>
                <td>&nbsp;</td>
                <td>@Html.LabelFor(model => model.Customer_name, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                <td> @Html.DisplayFor(model => model.Customer_name, new { htmlAttributes = new { @diabled = true } })</td>
            </tr>
            <tr><td>&nbsp;</td></tr>
            <tr>
                <td>@Html.LabelFor(model => model.Subscriber_range_start, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                <td> @Html.DisplayFor(model => model.Subscriber_range_start, new { htmlAttributes = new { @disabled = true } })</td>
                <td>&nbsp;</td>
                <td>@Html.LabelFor(model => model.Subscriber_range_end, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                <td> @Html.DisplayFor(model => model.Subscriber_range_end, new { htmlAttributes = new { @disabled = true } })</td>
            </tr>
            <tr><td>&nbsp;</td></tr>
            <tr>
                <td>@Html.LabelFor(model => model.Date, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                <td> @Html.DisplayFor(model => model.Date, new { htmlAttributes = new { @class = "form-control", @disabled = true } })</td>
                <td>&nbsp;</td>
                <td>@Html.LabelFor(model => model.Status, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                <td> @Html.CheckBoxFor(model => model.Status, new { htmlAttributes = new { @class = "form-control" } })</td>
            </tr>
            <tr><td>&nbsp;</td></tr>
            <tr>
                <td>@Html.LabelFor(model => model.CSVFileId, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                <td>@Html.DisplayFor(model => model.CSVFileId, new { htmlAttributes = new { @class = "form-control" } })</td>
                <td>&nbsp;</td>
                <td>@Html.LabelFor(model => model.UserId, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                <td>@Html.DisplayFor(model => model.UserId, new { htmlAttributes = new { @class = "form-control", @disbaled = true } })</td>
            </tr>
            <tr><td>&nbsp;</td></tr>
            <tr><td>&nbsp;</td></tr>

        </table>

    </div>

}

    
