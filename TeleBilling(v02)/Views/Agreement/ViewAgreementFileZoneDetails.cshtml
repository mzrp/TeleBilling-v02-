@model IEnumerable<TeleBilling_v02_.Models.ZoneRecords>
<h5>Supplier</h5>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Call_price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Minute_price)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.Country_code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CSVFileId)
        </th>*@
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.EditorFor(modelItem => item.Call_price, new { htmlAttributes = new { @disabled = true } })
        <td>
            @Html.EditorFor(modelItem => item.Minute_price, new { htmlAttributes = new { @disabled = true } })
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.Country_code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CSVFileId)
        </td>*@
    </tr>
}

</table>
